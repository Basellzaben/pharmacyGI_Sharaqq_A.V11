<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GalaxyMainActivity"
    android:background="#282b30"
    android:id="@+id/R_Back">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:weightSum="5"

        android:layoutDirection="ltr">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_gravity="center_horizontal"
            android:background="@color/Blue"
            android:weightSum="4"
            android:layout_height="50dp">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/Frag1"></FrameLayout>
        </LinearLayout>

        <RelativeLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_gravity="center_horizontal"
            android:layout_weight="0.4"
            android:background="#fff"
            android:gravity="right">

            <RelativeLayout
                android:layout_height="match_parent"
                android:layout_marginEnd="215dp"
                android:layout_width="250dp"
                android:background="@mipmap/row2"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:gravity="center">

                <Methdes.MyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="@string/Man_Balance"
                    android:textColor="#fff"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true" />

            </RelativeLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="right"
                android:background="@mipmap/row1"
                android:gravity="center"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true">

                <Methdes.MyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="@string/Main_Page"
                    android:id="@+id/textView4"
                    android:textColor="#fff" />
            </LinearLayout>

        </RelativeLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="4.2"
            android:weightSum="3.4">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="0.4"
                android:background="@color/Gray2"
                android:weightSum="2">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center">

                    <TextView
                        android:layout_height="40dp"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="123456789"
                        android:id="@+id/tv_RowCount"
                        android:maxLength="9"
                        android:gravity="center"
                        android:background="@drawable/btn_cir_white_fill_black"
                        android:layout_marginRight="5dp"
                        android:textSize="15sp"
                        android:layout_width="300dp" />

                    <Methdes.MyTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="@string/Number_Of_Item"
                        android:id="@+id/textView241"
                        android:textSize="15dp"
                        android:layout_marginLeft="20dp" />
                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center">

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_height="40dp"
                        android:background="@drawable/btn_cir_white_fill_black"
                        android:layout_width="280dp">

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:weightSum="2"
                            android:background="@drawable/btn_cir_white_fill_black">

                            <LinearLayout
                                android:orientation="horizontal"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1.5"
                                android:gravity="center"
                                >

                                <EditText
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textAppearance="?android:attr/textAppearanceMedium"
                                    android:text="1234567"
                                    android:id="@+id/et_OrdeNo"
                                    android:textSize="15sp"
                                    android:background="#fff" />
                            </LinearLayout>

                            <LinearLayout
                                android:orientation="horizontal"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="0.5"
                                android:gravity="center"
                                android:padding="5dp">

                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:id="@+id/imageButton"
                                    android:src="@mipmap/ic_search"
                                    android:onClick="btn_Search_Orders" />
                            </LinearLayout>
                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="0">

                        <Methdes.MyTextView
                            android:layout_height="wrap_content"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:text="@string/Order_ID"
                            android:id="@+id/textView240"
                            android:gravity="center"
                            android:textSize="15dp"
                            android:layout_marginLeft="10dp"
                            android:layout_width="70dp" />
                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="3"
                android:weightSum="2"
                android:padding="10dp">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="50dp"
                    android:gravity="center_vertical"
                    android:weightSum="9"
                    android:background="@color/Gray5">

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0.9"
                        android:background="@color/Black11"
                        android:layout_marginRight="1dp"
                        android:gravity="center"
                        android:padding="10dp">

                    </LinearLayout>

                    <Methdes.MyTextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:id="@+id/tv_diff"
                        android:textColor="#fff"
                        android:text="الفرق"
                        android:gravity="center"
                        android:layout_weight="0.9"
                        android:background="@color/Black11"
                        android:layout_marginLeft="1dp"
                        android:layout_marginRight="1dp"
                        android:textSize="16sp" />

                    <Methdes.MyTextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="الكمية الفعلية"
                        android:id="@+id/tv_ac_qty"
                        android:textColor="#fff"
                        android:gravity="center"
                        android:layout_weight="0.9"
                        android:background="@color/Black11"
                        android:layout_marginLeft="1dp"
                        android:layout_marginRight="1dp"
                        android:textSize="16sp" />

                    <Methdes.MyTextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="كمية المستودع"
                        android:id="@+id/tv_Qty"
                        android:gravity="center"
                        android:textColor="#fff"
                        android:layout_weight="0.9"
                        android:background="@color/Black11"
                        android:layout_marginLeft="1dp"
                        android:layout_marginRight="1dp"
                        android:textSize="16sp" />

                    <Methdes.MyTextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="الكمية المباعة"
                        android:id="@+id/tv_qtysaled"
                        android:gravity="center"
                        android:textColor="#fff"
                        android:layout_weight="0.9"
                        android:background="@color/Black11"
                        android:layout_marginLeft="1dp"
                        android:layout_marginRight="1dp"
                        android:textSize="16sp" />

                    <Methdes.MyTextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="كمية التزويد"
                        android:id="@+id/tv_QtyAcc"
                        android:gravity="center"
                        android:textColor="#fff"
                        android:layout_weight="0.9"
                        android:background="@color/Black11"
                        android:layout_marginLeft="1dp"
                        android:layout_marginRight="1dp"
                        android:textSize="16sp" />

                    <Methdes.MyTextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="الوحدة"
                        android:id="@+id/tv_UnitName"
                        android:gravity="center"
                        android:textColor="#fff"
                        android:layout_weight="0.9"
                        android:background="@color/Black11"
                        android:layout_marginLeft="1dp"
                        android:layout_marginRight="1dp"
                        android:textSize="16sp" />

                    <Methdes.MyTextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="اسم  المادة"
                        android:id="@+id/tv_ItemNm"
                        android:gravity="center"
                        android:textColor="#fff"
                        android:layout_weight="1.8"
                        android:background="@color/Black11"
                        android:layout_marginLeft="1dp"
                        android:layout_marginRight="1dp"
                        android:textSize="16sp" />

                    <Methdes.MyTextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="رقم المادة"
                        android:id="@+id/tv_Item_No"
                        android:gravity="center"
                        android:textColor="#fff"
                        android:layout_weight="0.9"
                        android:background="@color/Black11"
                        android:layout_marginLeft="1dp"
                        android:textSize="16sp" />

                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_weight="2"
                    android:layout_height="0dp"
                    android:background="@color/Main_Gray">

                    <ListView
                        android:layout_width="match_parent"
                        android:id="@+id/lst_Items"
                        android:smoothScrollbar="true"
                        android:stackFromBottom="false"
                        android:scrollingCache="false"
                        android:listSelector="@null"
                        android:layout_weight="1.7"
                        android:layout_height="match_parent" />
                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.4"
            android:weightSum="6"
            android:background="@color/Gray3">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/Footer_Menu"
                android:layout_marginRight="0.5dp"
                android:gravity="center"
                android:onClick="btn_back"
                android:id="@+id/Home_layout">

                <Methdes.MyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="@string/Home"
                    android:id="@+id/textView238"
                    android:textSize="18sp"
                    android:textColor="#fff"
                    android:layout_marginRight="12dp" />

                <ImageView
                    android:id="@+id/imageView20"
                    android:src="@mipmap/ic_home"
                    android:layout_width="25dp"
                    android:layout_height="25dp" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/Footer_Menu"
                android:layout_marginLeft="0.5dp"
                android:layout_marginRight="0.5dp"
                android:gravity="center"
                android:onClick="btn_ConvertToInvoice">

                <Methdes.MyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="@string/Sale_invoice"
                    android:id="@+id/textView237"
                    android:textSize="18sp"
                    android:textColor="#fff"
                    android:layout_marginRight="12dp" />

                <ImageView
                    android:id="@+id/imageView19"
                    android:src="@mipmap/ic_post"
                    android:layout_width="25dp"
                    android:layout_height="25dp" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/Footer_Menu"
                android:layout_marginLeft="0.5dp"
                android:layout_marginRight="0.5dp"
                android:gravity="center"
                android:onClick="btn_print">

                <Methdes.MyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="@string/Print"
                    android:id="@+id/textView236"
                    android:textSize="18sp"
                    android:textColor="#fff"
                    android:layout_marginRight="12dp" />

                <ImageView
                    android:id="@+id/imageView18"
                    android:src="@mipmap/ic_print"
                    android:layout_width="25dp"
                    android:layout_height="25dp" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/Footer_Menu"
                android:layout_marginLeft="0.5dp"
                android:layout_marginRight="0.5dp"
                android:gravity="center"
                android:onClick="btn_delete">

                <Methdes.MyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:id="@+id/textView233"
                    android:textSize="18sp"
                    android:textColor="#fff"
                    android:text="@string/Delete"
                    android:layout_marginRight="12dp" />

                <ImageView
                    android:id="@+id/imageView17"
                    android:src="@mipmap/ic_delete"
                    android:layout_width="25dp"
                    android:layout_height="25dp" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/Footer_Menu"
                android:layout_marginLeft="0.5dp"
                android:layout_marginRight="0.5dp"
                android:gravity="center"
                android:onClick="btn_save_po">

                <Methdes.MyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="@string/Save"
                    android:id="@+id/textView231"
                    android:textSize="18sp"
                    android:textColor="#fff"
                    android:layout_marginRight="12dp" />

                <ImageView
                    android:id="@+id/imageView16"
                    android:src="@mipmap/ic_save"
                    android:layout_width="25dp"
                    android:layout_height="25dp" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/Footer_Menu"
                android:layout_marginLeft="0.5dp"
                android:gravity="center"
                android:onClick="btn_new">

                <Methdes.MyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="@string/New"
                    android:id="@+id/textView230"
                    android:textSize="18sp"
                    android:textColor="#fff"
                    android:layout_marginRight="12dp" />

                <ImageView
                    android:id="@+id/imageView13"
                    android:src="@mipmap/ic_new"
                    android:layout_width="25dp"
                    android:layout_height="25dp" />
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>
</RelativeLayout>

