<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GalaxyMainActivity"
    android:background="#282b30"
    android:id="@+id/R_Back"
    android:fillViewport="true"
    android:layoutDirection="ltr">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/LL2"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_gravity="center_horizontal"
            android:background="@color/Blue"
            android:orientation="horizontal"
            android:weightSum="4">

            <FrameLayout
                android:id="@+id/Frag1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"></FrameLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/main"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/LL2"
            android:orientation="vertical"
            android:weightSum="5">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_gravity="center_horizontal"
                android:background="#fff"
                android:gravity="right"
                android:orientation="horizontal">

                <RelativeLayout
                    android:layout_width="250dp"
                    android:layout_height="match_parent"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginEnd="215dp"
                    android:background="@mipmap/row2"
                    android:gravity="center">

                    <Methdes.MyTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="طلب العينات"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="#fff" />

                </RelativeLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentTop="true"
                    android:layout_gravity="right"
                    android:background="@mipmap/row1"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <Methdes.MyTextView
                        android:id="@+id/textView4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/Main_Page"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="#fff" />
                </LinearLayout>

            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="4"
                android:orientation="vertical"
                android:weightSum="3">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.4"
                    android:background="@color/Gray2"
                    android:orientation="horizontal"
                    android:weightSum="3">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="20dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:weightSum="3">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="40dp"
                            android:layout_weight="2"
                            android:background="@drawable/btn_cir_white_fill_black"
                            android:onClick="btn_Search_Serial"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageView28"
                                android:layout_width="50dp"
                                android:layout_height="match_parent"
                                android:layout_margin="5dp"
                                android:src="@mipmap/ic_search" />

                            <Methdes.MyTextView
                                android:id="@+id/tv_serialDesc"
                                android:layout_width="0dp"
                                android:layout_height="40dp"
                                android:layout_weight="1.7"
                                android:gravity="center"
                                android:text=" "
                                android:textColor="#000"
                                android:textSize="16sp" />

                            <Methdes.MyTextView
                                android:id="@+id/tv_serialNo"
                                android:layout_width="0dp"
                                android:layout_height="40dp"
                                android:layout_weight="0.3"
                                android:gravity="center"
                                android:textColor="#000" />

                        </LinearLayout>

                        <Methdes.MyTextView
                            android:id="@+id/tv_serialNm"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="left|center_vertical"
                            android:text="التسلسل"
                            android:textColor="#000"
                            android:textSize="17sp" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:layoutDirection="rtl"
                        android:onClick="btn_Search_Orders"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="10dp"
                            android:layout_weight="0.4"
                            android:gravity="left|center_vertical"
                            android:orientation="vertical">

                            <Methdes.MyTextView
                                android:id="@+id/textView240"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="@string/Order_ID"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColor="#000"
                                android:textSize="15dp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="40dp"
                            android:layout_weight="0.6"
                            android:background="@drawable/btn_cir_white_fill_black"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@drawable/btn_cir_white_fill_black"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:layout_width="120dp"
                                    android:layout_height="match_parent"
                                    android:layout_margin="3dp"
                                    android:gravity="center"
                                    android:orientation="horizontal">

                                    <EditText
                                        android:id="@+id/et_OrdeNo"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:background="#fff"
                                        android:gravity="center"
                                        android:text="1234567"
                                        android:textAppearance="?android:attr/textAppearanceMedium"
                                        android:textColor="#000"
                                        android:textSize="15sp" />
                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/imageButton"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="0.7"
                                    android:gravity="center"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:layout_width="50dp"
                                        android:layout_height="match_parent"
                                        android:layout_margin="5dp"
                                        android:onClick="btn_Search_Orders"
                                        android:src="@mipmap/ic_search" />
                                </LinearLayout>
                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/fd"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginRight="20dp"
                        android:layout_weight="1"
                        android:layoutDirection="rtl"
                        android:orientation="horizontal">

                        <Methdes.MyTextView
                            android:id="@+id/textView239"
                            android:layout_width="100dp"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:layoutDirection="rtl"
                            android:text="التاريخ"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="#000"
                            android:textSize="15dp" />

                        <Methdes.MyTextView
                            android:id="@+id/tv_Date"
                            android:layout_width="110dp"
                            android:layout_height="40dp"
                            android:background="@drawable/btn_cir_white_fill_black"
                            android:ems="10"
                            android:gravity="center"
                            android:inputType="textPersonName"
                            android:text="01/01/2019"
                            android:textColor="#000" />

                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_marginTop="2dp"
                    android:background="@color/Gray2"
                    android:orientation="horizontal"
                    android:weightSum="2">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:layout_weight="1.5"
                        android:gravity="center_vertical|left"
                        android:orientation="horizontal"
                        android:weightSum="3">


                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_margin="2dp"
                            android:layout_marginLeft="30dp"
                            android:layout_weight="2"
                            android:orientation="horizontal"
                            android:paddingRight="20dp">

                            <EditText
                                android:id="@+id/Tv_Notes"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_marginRight="10dp"
                                android:layout_weight="1.8"
                                android:background="@drawable/btn_cir_white_fill_black"
                                android:ems="10"
                                android:gravity="right|center_vertical"
                                android:inputType="textPersonName"
                                android:paddingRight="10dp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <Methdes.MyTextView
                                android:id="@+id/textView269"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="1"
                                android:gravity="left|center_vertical"
                                android:text="البيان"
                                android:textColor="#000"
                                android:textSize="15sp" />
                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight=".5"
                        android:gravity="center|right"
                        android:onClick="btn_show_Pop1"
                        android:orientation="horizontal"
                        android:paddingRight="10dp">

                        <Methdes.MyTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="15dp"
                            android:text="@string/Add_item"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="#000" />

                        <ImageView
                            android:id="@+id/imageButton5"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_gravity="right"
                            android:src="@mipmap/ic_items" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="2.6"
                    android:orientation="vertical"
                    android:weightSum="3">

                    <LinearLayout
                        android:id="@+id/RR"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:background="@color/Gray1"
                        android:orientation="horizontal">

                        <ImageButton
                            android:id="@+id/imageButton14"
                            android:layout_width="45dp"
                            android:layout_height="50dp"
                            android:layout_gravity="center"
                            android:layout_marginRight="0.5dp"
                            android:background="#000"
                            android:scaleType="fitXY" />

                        <Methdes.MyTextView
                            android:id="@+id/tv_doctNm"
                            android:layout_width="fill_parent"
                            android:layout_height="50dp"
                            android:layout_gravity="center"
                            android:layout_marginLeft="0.5dp"
                            android:layout_marginRight="0.5dp"
                            android:layout_weight="1"
                            android:background="#17181c"
                            android:gravity="center"
                            android:text="اسم الطبيب"
                            android:textAlignment="center"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:textColor="#fff"
                            android:textSize="16sp" />

                        <Methdes.MyTextView
                            android:id="@+id/tv_doctNo"
                            android:layout_width="fill_parent"
                            android:layout_height="50dp"
                            android:layout_gravity="center"
                            android:layout_marginLeft="0.5dp"
                            android:layout_marginRight="0.5dp"
                            android:layout_weight="1"
                            android:background="#17181c"
                            android:gravity="center"
                            android:text="رقم الطبيب"
                            android:textAlignment="center"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:textColor="#fff"
                            android:textSize="16sp" />

                        <Methdes.MyTextView
                            android:id="@+id/tv_Unit"
                            android:layout_width="fill_parent"
                            android:layout_height="50dp"
                            android:layout_gravity="center"
                            android:layout_marginLeft="0.5dp"
                            android:layout_marginRight="0.5dp"
                            android:layout_weight="1"
                            android:background="#17181c"
                            android:gravity="center"
                            android:text="@string/Unit"
                            android:textAlignment="center"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:textColor="#fff"
                            android:textSize="16sp" />

                        <Methdes.MyTextView
                            android:id="@+id/tv_Qty"
                            android:layout_width="fill_parent"
                            android:layout_height="50dp"
                            android:layout_gravity="center"
                            android:layout_marginLeft="0.5dp"
                            android:layout_marginRight="0.5dp"
                            android:layout_weight="1"
                            android:background="#17181c"
                            android:gravity="center"
                            android:text="@string/Quentity"
                            android:textColor="#fff"
                            android:textIsSelectable="false"
                            android:textSize="16sp" />

                        <Methdes.MyTextView
                            android:id="@+id/tv_name"
                            android:layout_width="300dp"
                            android:layout_height="50dp"
                            android:layout_gravity="center"
                            android:layout_marginLeft="0.5dp"
                            android:layout_marginRight="0.5dp"
                            android:background="#17181c"
                            android:gravity="center"
                            android:text="@string/Item_Name"
                            android:textColor="#fff"
                            android:textIsSelectable="false"
                            android:textSize="16sp" />

                        <Methdes.MyTextView
                            android:id="@+id/tv_no"
                            android:layout_width="fill_parent"
                            android:layout_height="50dp"
                            android:layout_gravity="center"
                            android:layout_marginLeft="0.5dp"
                            android:layout_weight="1"
                            android:background="#17181c"
                            android:gravity="center"
                            android:text="@string/item_no"
                            android:textColor="#fff"
                            android:textSize="16sp" />


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="50dp"
                        android:background="@color/Main_Gray"
                        android:orientation="horizontal">

                        <ListView
                            android:id="@+id/LstvItems"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1.7"
                            android:listSelector="@null"
                            android:scrollingCache="false"
                            android:smoothScrollbar="true"
                            android:stackFromBottom="false" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_marginTop="-50dp"
                        android:layout_weight="1"
                        android:background="@color/Gray2"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:weightSum="4">

                        <TextView
                            android:id="@+id/tv_cusnm"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:text="1234567"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textSize="15sp"
                            android:visibility="invisible" />

                        <TextView
                            android:id="@+id/tv_acc"
                            android:layout_width="10dp"
                            android:layout_height="40dp"
                            android:layout_marginRight="5dp"
                            android:layout_weight="2"
                            android:background="@drawable/btn_cir_white_fill_black"
                            android:gravity="center"
                            android:maxLength="9"
                            android:text="123456789"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textSize="15sp"
                            android:visibility="invisible" />

                        <Methdes.MyTextView_Digital
                            android:id="@+id/et_Total"
                            android:layout_width="30dp"
                            android:layout_height="match_parent"
                            android:layout_marginBottom="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="0.2"
                            android:background="@drawable/btn_cir_white_fill_black"
                            android:gravity="center"
                            android:text="0"
                            android:textColor="#000"
                            android:textSize="22sp" />

                        <Methdes.MyTextView
                            android:id="@+id/textView243"
                            android:layout_width="0dp"
                            android:layout_height="45dp"
                            android:layout_weight="0.5"
                            android:gravity="center"
                            android:text="عدد المواد"
                            android:textColor="#000"
                            android:textSize="18sp" />

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:layout_weight="1"
                android:background="@color/Gray3"
                android:orientation="horizontal"
                android:weightSum="5">

                <LinearLayout
                    android:id="@+id/imageButton7"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="0.5dp"
                    android:layout_weight="1"
                    android:background="@color/Footer_Menu"
                    android:gravity="center"
                    android:onClick="btn_back"
                    android:orientation="horizontal">

                    <Methdes.MyTextView
                        android:id="@+id/textView238"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="12dp"
                        android:text="@string/Home"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="#fff"
                        android:textSize="18sp" />

                    <ImageView
                        android:id="@+id/imageView20"
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@mipmap/ic_home" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/imageButton9"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.5dp"
                    android:layout_marginRight="0.5dp"
                    android:layout_weight="1"
                    android:background="@color/Footer_Menu"
                    android:gravity="center"
                    android:onClick="btn_share"
                    android:orientation="horizontal">

                    <Methdes.MyTextView
                        android:id="@+id/textView237"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="12dp"
                        android:text="@string/Post"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="#fff"
                        android:textSize="18sp" />

                    <ImageView
                        android:id="@+id/imageView19"
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@mipmap/ic_post" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.5dp"
                    android:layout_marginRight="0.5dp"
                    android:layout_weight="1"
                    android:background="@color/Footer_Menu"
                    android:gravity="center"
                    android:onClick="btn_delete"
                    android:orientation="horizontal">

                    <Methdes.MyTextView
                        android:id="@+id/textView233"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="12dp"
                        android:text="@string/Delete"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="#fff"
                        android:textSize="18sp" />

                    <ImageView
                        android:id="@+id/imageView17"
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@mipmap/ic_delete" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.5dp"
                    android:layout_marginRight="0.5dp"
                    android:layout_weight="1"
                    android:background="@color/Footer_Menu"
                    android:gravity="center"
                    android:onClick="btn_save_po"
                    android:orientation="horizontal">

                    <Methdes.MyTextView
                        android:id="@+id/textView231"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="12dp"
                        android:text="@string/Save"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="#fff"
                        android:textSize="18sp" />

                    <ImageView
                        android:id="@+id/imageView16"
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@mipmap/ic_save" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.5dp"
                    android:layout_weight="1"
                    android:background="@color/Footer_Menu"
                    android:gravity="center"
                    android:onClick="btn_new"
                    android:orientation="horizontal">

                    <Methdes.MyTextView
                        android:id="@+id/textView230"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="12dp"
                        android:text="@string/New"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="#fff"
                        android:textSize="18sp" />

                    <ImageView
                        android:id="@+id/imageView13"
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@mipmap/ic_new" />
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

</ScrollView>

