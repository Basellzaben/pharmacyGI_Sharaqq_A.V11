<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/R_Back"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#282b30"
    android:layoutDirection="ltr"
    android:orientation="vertical"
    tools:context=".GalaxyMainActivity">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/LL2"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_gravity="center_horizontal"
            android:background="@color/Blue"
            android:orientation="horizontal"
            android:weightSum="4">

            <FrameLayout
                android:id="@+id/Frag1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"></FrameLayout>
        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_gravity="center_horizontal"
            android:layout_weight="0.5"
            android:background="#fff"
            android:gravity="right"
            android:orientation="horizontal">

            <RelativeLayout
                android:layout_width="250dp"
                android:layout_height="match_parent"
                android:layout_alignParentEnd="true"
                android:layout_alignParentTop="true"
                android:layout_marginEnd="215dp"
                android:background="@mipmap/row2"
                android:gravity="center">

                <Methdes.MyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="إعدادات عامة"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#fff" />

            </RelativeLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentEnd="true"
                android:layout_alignParentTop="true"
                android:layout_gravity="right"
                android:background="@mipmap/row1"
                android:gravity="center"
                android:orientation="horizontal">

                <Methdes.MyTextView
                    android:id="@+id/textView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/Main_Page"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#fff" />
            </LinearLayout>

        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:background="@color/Gray2"
            android:layoutDirection="rtl"
            android:orientation="horizontal"
            android:weightSum="3">

            <view
                android:id="@+id/view18"
                class="Methdes.MyTextView"
                id="@+id/view17"
                layout_weight="1.8"
                layout_width="0dp"
                android:layout_width="130dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:textColor="#000"
                tools:text="اسم الطابعة" />

            <EditText
                android:id="@+id/EditTextAddressBT"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginBottom="15dp"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:layout_marginTop="15dp"
                android:layout_weight="1.8"
                android:background="@drawable/btn_cir_white_fill_black"
                android:ems="10"
                android:gravity="center_vertical|center"
                android:inputType="textPersonName" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/dd"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.1"
            android:orientation="horizontal"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:paddingTop="10dp"
            android:weightSum="2">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/White"
                android:orientation="vertical"
                android:padding="10dp"
                android:weightSum="2">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:weightSum="2">

                    <EditText
                        android:id="@+id/et_m6"
                        android:layout_width="0dp"
                        android:layout_height="60dp"
                        android:layout_marginBottom="10dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1.1"
                        android:background="@drawable/btn_cir_white_fill_black"
                        android:ems="10"
                        android:gravity="center"
                        android:inputType="textPersonName" />

                    <Methdes.MyTextView
                        android:id="@+id/myTextView7"
                        android:layout_width="0dp"
                        android:layout_height="60dp"
                        android:layout_weight="0.9"
                        android:gravity="center|right"
                        android:paddingRight="20dp"
                        android:text="بداية تسلسل  طلب عينات"
                        android:textColor="@color/Black11"
                        android:textSize="16sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:weightSum="2">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="60dp"
                        android:layout_marginBottom="10dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1.10"
                        android:background="@drawable/btn_cir_white_fill_black"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:padding="3dp">

                        <ImageView

                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:layout_marginLeft="10dp"
                            android:src="@mipmap/d2" />

                        <Spinner
                            android:id="@+id/sp_MobileType"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="#fff"
                            android:backgroundTint="#fff"
                            android:gravity="center"
                            android:textAlignment="center" />

                    </LinearLayout>

                    <Methdes.MyTextView
                        android:id="@+id/myTextView12"
                        android:layout_width="0dp"
                        android:layout_height="60dp"
                        android:layout_weight="0.9"
                        android:gravity="center|right"
                        android:paddingRight="20dp"
                        android:text="نوع جهاز التابلت"
                        android:textColor="@color/Black11"
                        android:textSize="16sp" />

                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"

                android:background="@color/White"
                android:orientation="vertical"
                android:padding="10dp"
                android:weightSum="2">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:weightSum="2">

                    <EditText
                        android:id="@+id/et_IP"
                        android:layout_width="0dp"
                        android:layout_height="60dp"
                        android:layout_marginBottom="10dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1.1"
                        android:background="@drawable/btn_cir_white_fill_black"
                        android:ems="10"
                        android:gravity="center"
                        android:inputType="textPersonName" />

                    <Methdes.MyTextView
                        android:id="@+id/myTextView13"
                        android:layout_width="0dp"
                        android:layout_height="60dp"
                        android:layout_weight="0.9"
                        android:gravity="center|right"
                        android:paddingRight="20dp"
                        android:text="Servier IP"
                        android:textColor="@color/Black11"
                        android:textSize="16sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:weightSum="2">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="60dp"
                        android:layout_marginBottom="10dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1.10"
                        android:background="@drawable/btn_cir_white_fill_black"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:padding="3dp">

                        <ImageView
                            android:id="@+id/imageView24"
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:layout_marginLeft="10dp"
                            android:src="@mipmap/d2" />

                        <Spinner
                            android:id="@+id/sp_Print_Type"
                            android:layout_width="0dp"
                            android:layout_height="60dp"
                            android:layout_weight="1"
                            android:background="#fff"
                            android:backgroundTint="#fff"
                            android:gravity="center"
                            android:textAlignment="center" />

                    </LinearLayout>

                    <Methdes.MyTextView
                        android:id="@+id/myTextView14"
                        android:layout_width="0dp"
                        android:layout_height="60dp"
                        android:layout_weight="0.9"
                        android:gravity="center|right"
                        android:paddingRight="20dp"
                        android:text="نوع الطابعة"
                        android:textColor="@color/Black11"
                        android:textSize="16sp" />

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="@color/Gray5"
            android:orientation="horizontal"
            android:weightSum="2">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="20dp"
                android:layout_marginLeft="180dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="20dp"
                android:layout_weight="1"
                android:background="@color/Pink"
                android:onClick="DeleteAll">

                <Methdes.MyTextView
                    android:id="@+id/btn_Delete"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"

                    android:gravity="center"
                    android:text="مسح كل البيانات"
                    android:textColor="@color/White"
                    android:textSize="16dp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="20dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="180dp"
                android:layout_marginTop="20dp"
                android:layout_weight="1"
                android:background="@color/Green"
                android:onClick="btn_Save">

                <Methdes.MyTextView
                    android:id="@+id/button25"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:gravity="center"
                    android:onClick="btn_Save"

                    android:text="حفظ"
                    android:textColor="@color/White"
                    android:textSize="16dp" />
            </RelativeLayout>

        </LinearLayout>
    </LinearLayout>
</ScrollView>
