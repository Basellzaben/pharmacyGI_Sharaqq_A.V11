<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GalaxyMainActivity"
    android:background="#282b30"
    android:id="@+id/R_Back"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:layoutDirection="locale">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:weightSum="5"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_gravity="center_horizontal"
            android:background="@color/Blue"
            android:weightSum="4"
            android:layout_height="50dp">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/Frag1"></FrameLayout>
        </LinearLayout>

        <RelativeLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:background="#fff"
            android:layout_height="50dp"
            android:id="@+id/Header">

            <RelativeLayout
                android:layout_height="match_parent"
                android:layout_width="250dp"
                android:background="@mipmap/row2en"
                android:layout_alignParentTop="false"
                android:layout_alignParentEnd="false"
                android:gravity="center"
                android:layout_marginStart="215dp"
                android:id="@+id/RaltiveRow2">

                <Methdes.MyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="@string/CountryReport"
                    android:textColor="#fff" />

            </RelativeLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="right"
                android:background="@mipmap/row1en"
                android:gravity="center"

                android:layout_alignParentEnd="false"
                android:id="@+id/LinearRow1">

                <Methdes.MyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="@string/Main_Page"
                    android:id="@+id/textView4"
                    android:textColor="#fff" />
            </LinearLayout>

        </RelativeLayout>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/Contant">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:background="@color/Gray2"
                android:weightSum="2"
                android:layout_height="90dp">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1.5">

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:weightSum="2"
                        android:layout_weight="1.5">

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1.2"
                            android:gravity="center"
                            android:layout_marginEnd="25dp">

                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:layout_weight="0.5">

                                <Methdes.MyTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textAppearance="?android:attr/textAppearanceMedium"
                                    android:text="@string/ManNm"
                                    android:gravity="center"
                                    android:textSize="15dp"
                                    android:textColor="#000" />
                            </LinearLayout>

                            <LinearLayout
                                android:orientation="horizontal"
                                android:layout_width="0dp"
                                android:background="@drawable/btn_cir_white_fill_black"
                                android:layout_weight="1"
                                android:layout_height="35dp">

                                <LinearLayout
                                    android:orientation="horizontal"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:weightSum="2"
                                    android:background="@drawable/btn_cir_white_fill_black"
                                    android:onClick="btn_searchMan"
                                    android:id="@+id/country">

                                    <LinearLayout
                                        android:orientation="horizontal"
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="0.4"
                                        android:gravity="center"
                                        android:padding="5dp">

                                        <ImageView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:src="@mipmap/ic_search"
                                           />
                                    </LinearLayout>

                                    <LinearLayout
                                        android:orientation="horizontal"
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1.6"
                                        android:gravity="center">

                                        <Methdes.MyTextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:textAppearance="?android:attr/textAppearanceMedium"
                                            android:textSize="15sp"
                                            android:id="@+id/tv_ManNm"
                                            android:textColor="#000" />
                                    </LinearLayout>

                                </LinearLayout>

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.8"
                            android:gravity="center"
                            android:layout_marginRight="25dp">

                            <Methdes.MyTextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:text="@string/From_Date"
                                android:textSize="15dp"
                                android:layout_weight="0.5"
                                android:gravity="center"
                                android:textColor="#000" />

                            <Methdes.MyTextView
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:background="@drawable/btn_cir_white_fill_black"
                                android:gravity="center"
                                android:layout_marginRight="8dp"
                                android:textSize="15sp"
                                android:layout_weight="1.5"
                                android:layout_width="0dp"
                                android:layout_height="35dp"
                                android:id="@+id/ed_FromDate"
                                android:text="01/01/2017"
                                android:inputType="date"
                                android:focusable="false"
                                android:focusableInTouchMode="false"
                                android:textColor="#000" />

                        </LinearLayout>

                    </LinearLayout>
                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:weightSum="2"
                        android:layout_weight="1.5">

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1.2"
                            android:gravity="center"
                            android:layout_marginEnd="25dp">

                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:layout_weight="0.5">

                                <Methdes.MyTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textAppearance="?android:attr/textAppearanceMedium"
                                    android:text="@string/CountryNm"
                                    android:id="@+id/textView240"
                                    android:gravity="center"
                                    android:textSize="15dp"
                                    android:textColor="#000" />
                            </LinearLayout>

                            <LinearLayout
                                android:orientation="horizontal"
                                android:layout_width="0dp"
                                android:background="@drawable/btn_cir_white_fill_black"
                                android:layout_weight="1"
                                android:layout_height="35dp">

                                <LinearLayout
                                    android:orientation="horizontal"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:weightSum="2"
                                    android:background="@drawable/btn_cir_white_fill_black"
                                    android:onClick="btn_CountrySearch">

                                    <LinearLayout
                                        android:orientation="horizontal"
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="0.4"
                                        android:padding="5dp"
                                        android:gravity="center">

                                        <ImageView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:src="@mipmap/ic_search" />
                                    </LinearLayout>

                                    <LinearLayout
                                        android:orientation="horizontal"
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1.6"
                                        android:gravity="center">

                                        <Methdes.MyTextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:textAppearance="?android:attr/textAppearanceMedium"
                                            android:text=" "
                                            android:id="@+id/tv_Area"
                                            android:textSize="20sp"
                                            android:textColor="#000" />
                                    </LinearLayout>

                                </LinearLayout>

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.8"
                            android:gravity="center"
                            android:layout_marginRight="25dp">

                            <Methdes.MyTextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:text="@string/To_Date"
                                android:id="@+id/textView239"
                                android:textSize="15dp"
                                android:layout_weight="0.5"
                                android:gravity="center"
                                android:textColor="#000" />

                            <Methdes.MyTextView
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:id="@+id/ed_ToDate"
                                android:background="@drawable/btn_cir_white_fill_black"
                                android:gravity="center"
                                android:layout_marginRight="8dp"
                                android:textSize="15sp"
                                android:layout_weight="1.5"
                                android:layout_width="0dp"
                                android:layout_height="35dp"
                                android:text="31/12/2017"
                                android:inputType="date"
                                android:focusableInTouchMode="false"
                                android:focusable="false"
                                android:textColor="#000" />

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5"
                    android:gravity="center"
                    android:weightSum="2">

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1">

                        <RadioGroup
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/G1"
                            android:weightSum="2"
                            android:orientation="horizontal"
                            android:layoutMode="opticalBounds"
                            android:layout_gravity="center_vertical">

                            <RadioButton
                                android:text="@string/yearfrom"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:id="@+id/rdoYearFrom"
                                android:layout_weight="1"
                                android:background="@color/Pink"
                                android:layout_margin="3dp"
                                android:onClick="btn_fromYear" />

                            <RadioButton
                                android:text="@string/frommonth"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:id="@+id/rdoMonthFrom"
                                android:layout_weight="1"
                                android:background="@color/Green"
                                android:layout_margin="3dp"
                                android:onClick="btn_fromMonth" />
                        </RadioGroup>
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:layout_margin="3dp">

                        <Button
                            style="?android:attr/buttonStyleSmall"
                            android:layout_height="wrap_content"
                            android:text="      استرجاع المعلومات   "
                            android:id="@+id/button17"
                            android:background="@color/Blue"
                            android:textColor="#fff"
                            android:onClick="btn_GetData"
                            android:textSize="16sp"
                            android:layout_width="180dp"
                            android:layout_weight="1" />
                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_weight="0"
                android:background="@color/Gray3"
                android:weightSum="4"
                android:paddingLeft="20dp"
                android:layout_height="40dp">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:layout_marginRight="25dp">

                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:layout_marginRight="25dp">

                </LinearLayout>


                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:layout_marginRight="25dp">

                </LinearLayout>


                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="right|center"
                    android:layout_marginRight="25dp">

                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="10dp"
                    android:background="@color/Main_Gray">

                    <ListView
                        android:id="@+id/lst_acc"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:listSelector="@null"
                        android:scrollingCache="false"
                        android:smoothScrollbar="true"
                        android:stackFromBottom="false" />
                </LinearLayout>

            </LinearLayout>

        </LinearLayout>
    </LinearLayout>
</RelativeLayout>
